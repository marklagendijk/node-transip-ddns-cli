{
  "name": "transip-dns-cli",
  "version": "1.0.4",
  "description": "Node.js cli tool for updating TransIP DNS entries.",
  "bin": {
    "transip-dns-cli": "bin/cli.js"
  },
  "main": "bin/cli.js",
  "keywords": [
    "transip",
    "dns",
    "ddns",
    "dyndns",
    "dynamic"
  ],
  "author": "Mark Lagendijk <mark@lagendijk.nl>",
  "license": "MIT",
  "repository": "github:marklagendijk/transip-dns-cli",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "prettier --write . && eslint --cache --fix ."
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "got": "^11.8.5",
    "lodash": "^4.17.15",
    "parse-duration": "^0.1.2",
    "public-ip": "^4.0.0",
    "table": "^5.4.6",
    "uuid": "^7.0.2",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.js": "eslint --cache --fix"
  }
}
